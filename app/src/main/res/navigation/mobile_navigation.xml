<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_giftlist">

    <fragment
        android:id="@+id/navigation_giftlist"
        android:name="com.example.mypresent.ui.home.UebersichtGeschenkeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_uebersicht_geschenke" />

    <fragment
        android:id="@+id/navigation_shopping"
        android:name="com.example.mypresent.ui.dashboard.ShoppingFragment"
        android:label="title_shopping"
        tools:layout="@layout/fragment_shopping" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.mypresent.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>